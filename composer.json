{
  "name": "isemary/backup-sentry",
  "description": "BackupSentry is a PHP backup package designed to provide a secure and seamless solution for protecting and backup your critical data. With its advanced features and intuitive interface, Effortlessly create scheduled backups of databases, files, and directories.",
  "type": "library",
  "version": "1.0.2",
  "keywords": [
    "backup",
    "database-backup",
    "google-drive-backup",
    "php-backup",
    "laravel-backup-package",
    "backup-one-line",
    "aws-backup"
  ],
  "license": "MIT",
  "minimum-stability": "stable",
  "homepage": "https://www.abdelrahman.online",
  "autoload": {
    "psr-0": {
      "iSemary": "src/"
    },
    "psr-4": {
      "iSemary\\BackupSentry\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Abdelrahman Samir",
      "email": "abdelrahmansamirmostafa@gmail.com",
      "homepage": "https://www.abdelrahman.online",
      "role": "Developer"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "iSemary\\BackupSentry\\BackupSentryServiceProvider"
      ]
    }
  },
  "require": {
    "php": ">=7.4",
    "phpmailer/phpmailer": "^6.8",
    "vlucas/phpdotenv": "^5.5",
    "aws/aws-sdk-php": "^3.0",
    "illuminate/console": "^9.0|^10.0",
    "illuminate/support": "^9.0|^10.0",
    "guzzlehttp/guzzle": "^6.5.0|^7.0.0",
    "guzzlehttp/psr7": "^1.7.0|^2.0.0",
    "guzzlehttp/promises": "^1.4.0|^2.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.2"
  }
}
